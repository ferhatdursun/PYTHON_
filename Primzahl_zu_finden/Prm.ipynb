    "import math\n",
    "n = int(input(\"LÃ¼tfen bir sayi giriniz!\"))\n",
    "flag = True\n",
    "sqr = int(math.sqrt(n))\n",
    "\n",
    "if n<2:\n",
    "    flag = False\n",
    "\n",
    "for i in range(2,sqr+1):\n",
    "    if n%i==0:\n",
    "        flag = False\n",
    "\n",
    "if flag == False:\n",
    "    print(n,\"bir asal sayi degildir.\")\n",
    "\n",
